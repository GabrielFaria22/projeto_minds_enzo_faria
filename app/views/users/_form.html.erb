<%= form_with(model: user, local: true) do |form| %>
  

        <div class="main-panel">
          <!-- Navbar -->
              <div class="jumbotron jumbotron-sm">
          <div class="container">
              <div class="row">
                  <div class="col-sm-12 col-lg-12">
                      <h1 class="h1">
                          Dados do Assistente Social</h1>
                  </div>
              </div>
          </div>
      </div>
      <div class="container">
          <div class="row">
              <div class="col-md-8">
                  <div class="well well-sm">
                      <form>
                      <div class="row">
                          <div class="col-md-6">
                              <div class="form-group">
                                <%= form.text_field :email, class: 'form-control', placeholder: 'Email' %>
                              </div>
                              <% if @minimum_password_length %>
                              <em>(<%= @minimum_password_length %> characters minimum)</em>
                              <% end %><br />
                              <div class="form-group">
                                <%= form.password_field :password, class: 'form-control', placeholder: 'Senha' %>
                              </div>
                              <div class="form-group">
                                <%= form.password_field :password_confirmation, class: 'form-control', placeholder: 'Confirmação da senha' %>
                              </div>
                              <%= form.submit 'Confirmar', class: 'btn btn-primary' %>
                          </div>
                      </div>
                      </form>
                  </div>
              </div>

          </div>
      </div>
<% end %>
